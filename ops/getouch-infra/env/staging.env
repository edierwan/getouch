# Staging Environment
# Copy to /opt/getouch-stg/env/.env and fill in the values

CLOUDFLARE_TUNNEL_TOKEN_STG=eyJ...paste-staging-tunnel-token-here

# Database (SATA disk)
POSTGRES_USER=getouch
POSTGRES_PASSWORD=CHANGE_ME_staging_secret
POSTGRES_DB=getouch

# Service database URLs (point to postgres-stg container)
DATABASE_URL=postgresql://getouch:CHANGE_ME_staging_secret@postgres-stg:5432/getouch
DATABASE_URL_BOT=postgresql://getouch:CHANGE_ME_staging_secret@postgres-stg:5432/getouch_bot
DATABASE_URL_WA=postgresql://getouch:CHANGE_ME_staging_secret@postgres-stg:5432/getouch_wa

# Shared Ollama
OLLAMA_HOST=ollama
OLLAMA_PORT=11434
OLLAMA_MODEL=qwen2.5:7b

# pgAdmin
PGADMIN_EMAIL=admin@getouch.co
PGADMIN_PASSWORD=CHANGE_ME_pgadmin_secret

# General
NODE_ENV=staging
TZ=Asia/Kuala_Lumpur
API_PORT=3000
BOT_PORT=3000
WA_PORT=3000
LANDING_PORT=3000
